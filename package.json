{
  "name": "multiline-string-editor",
  "displayName": "Multiline String Editor",
  "description": "Visual Studio Code extension for editing multiline strings (e.g., in JSON).",
  "version": "0.0.2",
  "publisher": "publicus",
  "author": {
    "name": "The Children's Hospital of Philadelphia"
  },
  "license": "MIT",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "JSON"
  ],
  "markdown": "github",
  "activationEvents": [
    "onCommand:extension.editMultiline"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.editMultiline",
        "title": "Edit as multiline string"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/toiroakr/stringified-json-editor.git"
  },
  "homepage": "https://github.com/toiroakr/stringified-json-editor/blob/master/README.md"
}
